{
  "version": 2,
  "builds": [
    {
      "src": "BestPrice/Supermarket/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "BestPrice/build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "BestPrice/staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/BestPrice/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/BestPrice/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "BestPrice/Supermarket/wsgi.py"
    }
  ]
}